{
  "name": "f-app",
  "version": "0.0.1",
  "description": "Scaffolding for a basic Typescript+React frontend poject",
  "main": "dist/index.html",
  "repository": "git@github.com:asleepysamurai/ts-scaffold-f-app.git",
  "author": "Balaganesh Damodaran <asleepysamurai@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "x": " ",
    "env:types": "rimraf src/codegen && mkdir -p src/codegen && types-from-env ./config/default.env ./src/codegen/env.types.ts",
    "prep": "rimraf ./dist && mkdir -p ./dist && yarn env:types",
    "start": "yarn prep && NODE_ENV=development webpack-dev-server --config webpack.config.js --env.NODE_ENV=development",
    "start:inspect": "yarn start",
    "build": "yarn prep && NODE_ENV=production webpack --config webpack.config.js --env.NODE_ENV=production"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/html-webpack-plugin": "^3.2.3",
    "@types/node": "^14.0.6",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/webpack": "^4.41.16",
    "@types/webpack-dev-server": "^3.11.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.20.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "prettier-eslint-cli": "^5.0.0",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^7.0.5",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "engines": {
    "node": ">=14.2.0 <15.0.0"
  },
  "dependencies": {
    "bluejacket": "^0.0.11",
    "debug": "^4.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "types-from-env": "^1.0.0"
  }
}
